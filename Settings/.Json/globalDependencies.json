{
	"dependencies": [
		{
			"name": "libpcap",
			"download": "wget -O {{name}}/libpcap-1.9.1.tar.gz https://www.tcpdump.org/release/libpcap-1.9.1.tar.gz",
			"command": "tar zxvf libpcap-1.9.1.tar.gz && cd libpcap-1.9.1 && ./configure --prefix={{projectVendors}}/{{name}} && make && make install && cd ..",
			"includes": [
				"include/*"
			],
			"libs": [
				"lib/*"
			],
			"others": [
			],
			"reference": "https://www.tcpdump.org/release/",
			"remove": "rm -rf {{name}}",
			"windows": false,
			"linux": true
		},
		{
			"name": "cJson",
			"download": "git clone https://github.com/DaveGamble/cJSON.git {{name}}",
			"command": "make",
			"includes": [
				"cJSON.h"
			],
			"libs": [
				"libcjson.a"
			],
			"others": [
			],
			"reference": "https://github.com/DaveGamble/cJSON",
			"remove": "rm -rf {{name}}",
			"windows": true,
			"linux": true
		},
		{
			"name": "winDivert",
			"download": "wget -O {{name}}/tmp.zip https://github.com/basil00/WinDivert/releases/download/v2.2.2/WinDivert-2.2.2-A.zip",
			"command": "unzip tmp.zip",
			"includes": [
				"WinDivert-2.2.2-A/include/windivert.h"
			],
			"libs": [
				"WinDivert-2.2.2-A/x64/WinDivert.lib",
				"WinDivert-2.2.2-A/x64/WinDivert64.sys",
				"WinDivert-2.2.2-A/x64/WinDivert.dll"
			],
			"others": [
			],
			"reference": "https://github.com/basil00/WinDivert",
			"remove": "rm -rf {{name}}",
			"windows": true,
			"linux": false
		}
	]
}