{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Sizing GDB (unix)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/build/Tests/Models/Commons/Sources/Tests.Commons",
			"args": [""],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}/build",
			"environment": [
				{
					"name": "CB_DEBUG_OUTPUT",
					"value": "1"
				}				
			],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/bin/gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": false
				},
				{
				  "description": "Set breakpoint on main",
				  "text": "break main",
				  "ignoreFailures": false
				}
			]
		},
		{
			"name": "Sizing GDB (windows)",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/Bin/SizingMain.exe",
			"args": [""],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [
				
				{
					"name": "CB_DEBUG_OUTPUT",
					"value": "1"
				}				
			],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "C:\\msys64\\mingw64\\bin\\gdb.exe",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": false
				},
				{
				  "description": "Set breakpoint on main",
				  "text": "break main",
				  "ignoreFailures": false
				}
			]
		},
	]
}